'use strict';

const fs = require('fs')
    , cwd = process.cwd()
    , join = require('path').join
    , local = join(cwd, './node_modules/arduinodk/src/cli.js')
    , cli = fs.existsSync(local) ? local : '../src/cli.js'

require(cli).run(process.argv.slice(2))
